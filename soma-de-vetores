#include <iostream>
using namespace std;
int main()
{
  int i;
  int n;


  cin >> n;
  float A[n], B[n], C[n];



  for(i=0; i < n; i=i+1){

    cin >> A[i];
  }

  for(i=0; i < n; i=i+1){

    cin >> B[i];
  }
  for(i=0; i < n; i=i+1){
    C[i] = A[i] + B[i];
  }

  for(i=0; i < n; i=i+1){
	cout << C[i]<<" ";
  }

  cout << "\n";

  return 0;
}
